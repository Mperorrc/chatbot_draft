    <pattern>greeting</pattern>
    <template>
      <random>
        <li>Hi there!</li>
        <li>Hello, how can I help you?</li>
        <li>Greetings!</li>
        <li>Hi <star/>, nice to meet you!</li>
      </random>
    </template>
  </category>
  <category>
    <pattern>greeting *</pattern>
